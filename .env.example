# 日志设置
LOG_LEVEL=INFO
VERBOSE_LOGGING=1

# 超时设置（秒）
CONNECT_TIMEOUT=10.0
STREAM_TIMEOUT=600.0
MAX_RETRIES=3
RETRY_DELAY=1.0

# 缓冲区设置
BUFFER_SIZE=100
BUFFER_TIME=0.2

# API服务设置
BASE_URL=https://apps.abacus.ai

# 会话设置 - 支持多个会话配置，用逗号分隔
# 格式: DEPLOYMENT_ID_1,DEPLOYMENT_ID_2,DEPLOYMENT_ID_3
DEPLOYMENT_IDS=
# 格式: EXTERNAL_APP_ID_1,EXTERNAL_APP_ID_2,EXTERNAL_APP_ID_3
EXTERNAL_APP_IDS=
# 同一位置的ID会配对使用，例如 DEPLOYMENT_IDS[0] 会配合 EXTERNAL_APP_IDS[0]
NEW_CHAT_NAME=New Chat

# 请求头设置
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/133.0.0.0 Safari/537.36 Edg/133.0.0.0

# 并发和会话管理设置
MAX_SESSIONS=10          # 最大活跃会话数
SESSION_TIMEOUT=3600     # 会话超时时间(秒)
MAX_CONCURRENT_REQUESTS=50  # 最大并发请求数
ACCOUNT_SELECTION_STRATEGY=round_robin  # 账号选择策略: round_robin(轮询) 或 random(随机) 

INITIAL_SESSION_TOKEN=
